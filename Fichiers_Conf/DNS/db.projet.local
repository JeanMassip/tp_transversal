;fichier de zone classique pour la zone projet.local
;en correspondance avec la zone inversee 168.192.in-addr-arpa.
$TTL	86400

projet.local.	IN	SOA	gestinfra.projet.local. kossi.projet.local. (
			      10	; Serial
			 604800		; Refresh
			  30		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
projet.local.	IN	NS	gestinfra.projet.local.
vmubuntu.projet.local.	IN	A	192.168.1.1
vmubuntu.projet.local.	IN	A	192.168.0.1
passerelle.projet.local.	IN	A	192.168.1.2
passerelle.projet.local.	IN	A	192.168.0.2
gestinfra.projet.local.	IN	A	192.168.1.3
web.projet.local.	IN 	A	192.168.1.4
centraleven.projet.local.	IN 	A	192.168.1.5
filetampon.projet.local.	IN 	A	192.168.1.6
sauvegarde.projet.local.	IN 	A	192.168.1.7

